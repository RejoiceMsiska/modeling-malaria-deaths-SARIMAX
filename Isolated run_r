# Load required libraries
library(forecast)
library(ggplot2)
library(lubridate)
library(dplyr)

# Source train and predict scripts
source("train")
source("predict")

# Run train and predict functions
train("input/trainData.csv", "output/model.bin")
predict("output/model.bin", "input/trainData.csv", "input/futureClimateData.csv", "output/predictions.csv")
